{
  "version": 2,
  "functions": {
    "api/index.php": {
      "runtime": "vercel-php@0.7.1"
    }
  },
  "routes": [
    {
      "src": "/app.css",
      "dest": "/api/app.css"
    },
    {
      "src": "/logo-media-lens.png",
      "dest": "/api/logo-media-lens.png"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    }
  ]
}